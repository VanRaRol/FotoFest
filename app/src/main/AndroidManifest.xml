<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.oriolqa.foto_fest">

    <uses-permission android:name="android.permission.USE_FINGERPRINT" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyClf5BI63gZAXRDNErosRHLtJuRLQn00z8" />

    <activity
        android:name="com.example.oriolqa.foto_fest.MainActivity" />


    <activity android:name="com.example.oriolqa.foto_fest.LoginActivity"
        android:theme="@style/AppTheme.Dark"
        android:label="@string/app_name" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>



    <activity android:name="com.example.oriolqa.foto_fest.SignupActivity" android:theme="@style/AppTheme.Dark" />
        <activity android:name="com.example.oriolqa.foto_fest.SettingsActivity" android:theme="@style/AppTheme.Dark" />
        <activity android:name="com.example.oriolqa.foto_fest.ChannelDetailActivity"/>
        <activity android:name="com.example.oriolqa.foto_fest.MapsMarkerActivity"/>
        <activity android:name="com.example.oriolqa.foto_fest.CalendarActivity"/>
    </application>
</manifest>
